  "functional_assay_data": {
    "cell_proliferation": "RET p.S891A/p.R525W and p.S891A increased cell proliferation; p.S891A/p.R525W > p.S891A; p.R525W alone had no effect.",
    "akt_phosphorylation": "RET p.R525W, p.S891A, p.S891A/p.R525W, and p.C634Y increased Akt phosphorylation; p.R525W effect 20%-30% weaker than p.S891A/p.S891A/p.R525W/p.C634Y.",
    "caspase3_levels": "RET mutants downregulated caspase 3.",
    "glycosylation_status": "Complete inhibition of RET glycosylation by p.R525W, p.S891A, p.S891A/p.R525W, p.C634Y.",
    "dimerization_status": "Disulfide-bridge-mediated dimerization only in p.C634Y; p.S891A/p.R525W are monomeric.",
    "immunostaining_localization": "p.R525W, p.S891A, p.S891A/p.R525W mainly cytoplasmic; p.C634Y in membrane/cytoplasm."
  },
  "ca_phenotype_data": {
    "distribution": "Three cases in family (II-2, II-5, III-3), all with RET p.S891A and OSMR p.G513D; no other family members had CA.",
    "age_of_onset_per_patient": {
      "II-2": 28,
      "II-5": 27,
      "III-3": 31
    },
    "histology_findings": "Hyperkeratosis, mild acanthosis, elongated rete ridges, small globular amyloid deposits, crystal violet and Congo red positive.",
    "treatment": "Glucocorticoid cream; limited effect, application discontinued due to side effects."
  },
  "conclusions": {
    "mechanism": "RET p.S891A + OSMR p.G513D may underlie FMTC + CA phenotype",
    "co_segregation_pattern": "p.S891A follows MTC/CA; p.R525W does not co-segregate alone",
    "penetrance_notes": "Consistent with previous p.S891A FMTC families"
  },
